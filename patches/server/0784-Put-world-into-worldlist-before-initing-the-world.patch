             }
 
             worlddata.setModdedInfo(this.getServerModName(), this.getModdedStatus().shouldReportAsModified());
+            this.addLevel(world); // Paper - move up
             this.initWorld(world, worlddata, worldData, worldoptions);
 
-            this.addLevel(world);
