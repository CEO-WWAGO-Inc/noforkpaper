From 0000000000000000000000000000000000000000 Mon Sep 17 00:00:00 2001
From: Ben Kerllenevich <me@notom3ga.me>
Date: Sat, 9 Jan 2021 07:14:03 -0500
Subject: [PATCH] Add getServerProperties API


diff --git a/src/main/java/org/bukkit/Bukkit.java b/src/main/java/org/bukkit/Bukkit.java
index fecd7b14d317f55eb1ce7b5c6af9913917971427..3e12bb27ab56d7c54c77d8334dd977032c6b7a8b 100644
--- a/src/main/java/org/bukkit/Bukkit.java
+++ b/src/main/java/org/bukkit/Bukkit.java
@@ -1811,6 +1811,16 @@ public final class Bukkit {
     public static com.destroystokyo.paper.entity.ai.MobGoals getMobGoals() {
         return server.getMobGoals();
     }
+
+    /**
+     * Returns the {@link java.util.Properties} of the server.
+     *
+     * @return the server properties
+     */
+    @NotNull
+    public static java.util.Properties getServerProperties() {
+        return server.getServerProperties();
+    }
     // Paper end
 
     @NotNull
diff --git a/src/main/java/org/bukkit/Server.java b/src/main/java/org/bukkit/Server.java
index 8e01e1bbb7cfd98b47dbdb3a0e132dafd1413de6..0de1b8b92268630aed44ef87763b600e921bf90c 100644
--- a/src/main/java/org/bukkit/Server.java
+++ b/src/main/java/org/bukkit/Server.java
@@ -1581,5 +1581,13 @@ public interface Server extends PluginMessageRecipient {
      */
     @NotNull
     com.destroystokyo.paper.entity.ai.MobGoals getMobGoals();
+
+    /**
+     * Returns the {@link java.util.Properties} of the server.
+     *
+     * @return the server properties
+     */
+    @NotNull
+    java.util.Properties getServerProperties();
     // Paper end
 }
