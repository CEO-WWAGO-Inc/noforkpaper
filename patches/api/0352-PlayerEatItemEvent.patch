+package io.papermc.paper.event.packet;
+
+import org.bukkit.entity.Player;
+import org.bukkit.event.Cancellable;
+import org.bukkit.event.HandlerList;
+import org.bukkit.event.player.PlayerEvent;
+import org.bukkit.inventory.ItemStack;
+import org.jetbrains.annotations.NotNull;
+
+/**
+ * Fired when the player consumes an edible
+ * item.
+ */
+public class PlayerEatItemEvent extends PlayerEvent implements Cancellable {
+
+    private static final HandlerList HANDLERS = new HandlerList();
+
+    private boolean cancelled = false;
+
+    private float saturationModifier;
+    private int foodLevel;
+
+    private final ItemStack item;
+
+    public PlayerEatItemEvent(@NotNull Player what, int foodLevel, float saturationModifier, @NotNull ItemStack item) {
+        super(what);
+        this.foodLevel = foodLevel;
+        this.saturationModifier = saturationModifier;
+
+        this.item = item;
+    }
+
+    /**
+     * Gets the item that the player consumed in this event.
+     *
+     * @return consumed item
+     */
+    @NotNull
+    public ItemStack getItem() {
+        return item;
+    }
+
+    /**
+     * Gets the saturation modifier of this food item.
+     *
+     * @return saturation modifier
+     */
+    public float getSaturationModifier() {
+        return this.saturationModifier;
+    }
+
+    /**
+     * Sets the saturation modifier for this food item.
+     *
+     * @param saturationModifier new saturation modifier
+     */
+    public void setSaturationModifier(float saturationModifier) {
+        this.saturationModifier = saturationModifier;
+    }
+
+    /**
+     * Gets the food level of this food item.
+     *
+     * @return food level
+     */
+    public int getFoodLevel() {
+        return foodLevel;
+    }
+
+    /**
+     * Sets the food level of this food item.
+     *
+     * @param foodLevel new food level
+     */
+    public void setFoodLevel(int foodLevel) {
+        this.foodLevel = foodLevel;
+    }
+
+    @Override
+    public boolean isCancelled() {
+        return cancelled;
+    }
+
+    @Override
+    public void setCancelled(boolean cancel) {
+        this.cancelled = cancel;
+    }
+
+    @Override
+    public @NotNull HandlerList getHandlers() {
+        return HANDLERS;
+    }
+
+    @NotNull
+    public static HandlerList getHandlerList() {
+        return HANDLERS;
+    }
+}
