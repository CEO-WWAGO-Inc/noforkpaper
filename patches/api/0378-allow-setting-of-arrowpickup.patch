@@ -2,6 +2,7 @@ package org.bukkit.entity;
 
 import java.util.List;
 import org.bukkit.Color;
+import org.bukkit.inventory.ItemStack;
 import org.bukkit.potion.PotionData;
 import org.bukkit.potion.PotionEffect;
 import org.bukkit.potion.PotionEffectType;
@@ -94,4 +95,11 @@ public interface Arrow extends AbstractArrow {
      * in a state with no Custom Effects and PotionType.UNCRAFTABLE
      */
     void clearCustomEffects();
+
+    /**
+     * Sets the ItemStack given when the arrow is picked up.
+     *
+     * @param itemStack
+     */
+    void setPickupItem(ItemStack itemStack);
 }
