Subject: [PATCH] Expose CommandDispatcher


diff --git a/src/main/java/net/minecraft/server/CommandDispatcher.java b/src/main/java/net/minecraft/server/CommandDispatcher.java
index 2414b0a55..af1155cdb 100644
--- a/src/main/java/net/minecraft/server/CommandDispatcher.java
+++ b/src/main/java/net/minecraft/server/CommandDispatcher.java
@@ -349,7 +349,7 @@ public class CommandDispatcher {
         };
     }
 
-    public com.mojang.brigadier.CommandDispatcher<CommandListenerWrapper> a() {
+    public com.mojang.brigadier.CommandDispatcher<CommandListenerWrapper> a() { return getDispatcher(); } public com.mojang.brigadier.CommandDispatcher<CommandListenerWrapper> getDispatcher() { // Paper - OBFHELPER
         return this.b;
     }
 
diff --git a/src/main/java/org/bukkit/craftbukkit/CraftServer.java b/src/main/java/org/bukkit/craftbukkit/CraftServer.java
index dcc44be61..12a00a422 100644
--- a/src/main/java/org/bukkit/craftbukkit/CraftServer.java
+++ b/src/main/java/org/bukkit/craftbukkit/CraftServer.java
@@ -2222,5 +2222,10 @@ public final class CraftServer implements Server {
     public boolean isStopping() {
         return net.minecraft.server.MinecraftServer.getServer().hasStopped();
     }
+
+    @Override
+    public com.mojang.brigadier.CommandDispatcher getCommandDispatcher() {
+        return net.minecraft.server.MinecraftServer.getServer().commandDispatcher.getDispatcher();
+    }
     // Paper end
 }
