diff --git a/src/main/java/com/destroystokyo/paper/event/server/WhitelistToggleEvent.java b/src/main/java/com/destroystokyo/paper/event/server/WhitelistToggleEvent.java
index fdd5eedb2b7401439912a3a4343a920f32edc860..48964455611a2db172020cd63d4516d3a2e22dd2 100644
--- a/src/main/java/com/destroystokyo/paper/event/server/WhitelistToggleEvent.java
+++ b/src/main/java/com/destroystokyo/paper/event/server/WhitelistToggleEvent.java
@@ -1,9 +1,13 @@
 package com.destroystokyo.paper.event.server;
 
+import org.bukkit.Bukkit;
+import org.bukkit.OfflinePlayer;
 import org.bukkit.event.Event;
 import org.bukkit.event.HandlerList;
 import org.jetbrains.annotations.NotNull;
 
+import java.util.Set;
+
 /**
  * This event is fired when whitelist is toggled
  *
@@ -27,6 +31,15 @@ public class WhitelistToggleEvent extends Event {
         return enabled;
     }
 
+    /**
+     * Gets the currently whitelisted players from the server-whitelist
+     *
+     * @return Currently whitelisted players from the server-whitelist
+     */
+    public Set<OfflinePlayer> getWhitelistedPlayers() {
+        return Bukkit.getWhitelistedPlayers();
+    }
+
     @NotNull
     @Override
     public HandlerList getHandlers() {
