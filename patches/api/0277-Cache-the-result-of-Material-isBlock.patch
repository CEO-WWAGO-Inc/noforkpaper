From: Jason Penilla <11360596+jpenilla@users.noreply.github.com>
Date: Tue, 2 Mar 2021 15:24:58 -0800
Subject: [PATCH] Cache the result of Material#isBlock


diff --git a/src/main/java/org/bukkit/Material.java b/src/main/java/org/bukkit/Material.java
index ed35b7f47adad5e7001fa87b51d83144bc436243..802860178bf2342988182a3574636cebf5bcb58d 100644
--- a/src/main/java/org/bukkit/Material.java
+++ b/src/main/java/org/bukkit/Material.java
@@ -3933,6 +3933,7 @@ public enum Material implements Keyed, net.kyori.adventure.translation.Translata
     public final Class<?> data;
     private final boolean legacy;
     private final NamespacedKey key;
+    private boolean isBlock; // Paper
 
     private Material(final int id) {
         this(id, 64);
@@ -4140,6 +4141,11 @@ public enum Material implements Keyed, net.kyori.adventure.translation.Translata
      * @return true if this material is a block
      */
     public boolean isBlock() {
+    // Paper start - cache isBlock
+        return this.isBlock;
+    }
+    private boolean isBlock0() {
+    // Paper end
         switch (this) {
             //<editor-fold defaultstate="collapsed" desc="isBlock">
             case ACACIA_BUTTON:
@@ -5221,6 +5227,7 @@ public enum Material implements Keyed, net.kyori.adventure.translation.Translata
     static {
         for (Material material : values()) {
             BY_NAME.put(material.name(), material);
+            material.isBlock = material.isBlock0(); // Paper
         }
     }
 
