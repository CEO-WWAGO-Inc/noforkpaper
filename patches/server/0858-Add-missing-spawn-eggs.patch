+            Material.SNOW_GOLEM_SPAWN_EGG,
+            Material.WARDEN_SPAWN_EGG,
+            Material.WITHER_SPAWN_EGG,
+            // Paper end
             Material.AXOLOTL_SPAWN_EGG,
             Material.BAT_SPAWN_EGG,
             Material.BEE_SPAWN_EGG,
