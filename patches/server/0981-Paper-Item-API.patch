+    private static final AdapterContext CONTEXT = AdapterContext.of((context) -> {
+        context.register(TRIM_MATERIAL, new KeyedRegistryElementNbt<>(namespacedKey -> Bukkit.getRegistry(TrimMaterial.class).get(namespacedKey), TrimMaterial::getKey));
+        context.register(TRIM_PATTERN, new KeyedRegistryElementNbt<>(namespacedKey -> Bukkit.getRegistry(TrimPattern.class).get(namespacedKey), TrimPattern::getKey));
+    });
+
+    public ArmorTrimNbt() {
+        super(CompoundTagPrimitive.INSTANCE);
+    }
+
+    @Override
+    public ArmorTrim fromPrimitive(CompoundTag primitive) {
+        NbtPropertyHolder holder = NbtPropertyHolder.simpleBlob(primitive, CONTEXT);
+        return new ArmorTrim(holder.get(TRIM_MATERIAL), holder.get(TRIM_PATTERN));
+    }
+
+    @Override
+    public CompoundTag toPrimitive(ArmorTrim type) {
