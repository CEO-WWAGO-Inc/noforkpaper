diff --git a/src/main/java/org/bukkit/entity/Arrow.java b/src/main/java/org/bukkit/entity/Arrow.java
index 8814519df13c5e9475774f4bce4c8c6a462c2978..7f80a6d1a0f1f2d1889062a1f451e311bdc92e1a 100644
